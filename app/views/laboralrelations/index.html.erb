<p id="notice"><%= notice %></p>

<h1>Laboralrelations</h1>

<table>
  <thead>
    <tr>
      <th>Undefined</th>
      <th>Date ini</th>
      <th>Date finish</th>
      <th>Base salary</th>
      <th>Status</th>
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @laboralrelations.each do |laboralrelation| %>
      <tr>
        <td><%= laboralrelation.undefined %></td>
        <td><%= laboralrelation.date_ini %></td>
        <td><%= laboralrelation.date_finish %></td>
        <td><%= laboralrelation.base_salary %></td>
        <td><%= laboralrelation.status %></td>
        <td><%= link_to 'Show', laboralrelation %></td>
        <td><%= link_to 'Edit', edit_laboralrelation_path(laboralrelation) %></td>
        <td><%= link_to 'Destroy', laboralrelation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div class="employee-form">
    <%= paginate @laboralrelation %>
    <%= link_to 'Home', homes_index_path, class:"btn btn-outline-light btn-primary text-center" %>
</div>